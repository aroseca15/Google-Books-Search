{
  "name": "google-books-search",
  "version": "1.0.0",
  "description": "Book description  search and save.",
  "main": "server.js",
  "dependencies": {
    "axios": "^0.21.1",
    "concurrently": "^5.3.0",
    "express": "^4.17.1",
    "heroku": "^7.47.12",
    "if-env": "^1.0.4",
    "mongoose": "^5.11.15",
    "nodemon": "^2.0.7",
    "react-router-dom": "^5.2.0"
  },
  "devDependencies": {},
  "scripts": {
    "start": "if-env NODE_ENV=production && npm run start:prod || npm run start:dev",
    "start:prod": "node server.js",
    "start:dev": "concurrently \"nodemon --ignore 'client/*'\" \"npm run client\"",
    "client": "cd my-booksearch && npm run start",
    "seed": "node scripts/seedDB.js",
    "install": "cd client && npm install",
    "build": "cd client && npm run build",
    "heroku-postbuild": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/aroseca15/Google-Books-Search.git"
  },
  "keywords": [
    "run",
    "start"
  ],
  "author": "Amanda MacDonald",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/aroseca15/Google-Books-Search/issues"
  },
  "homepage": "https://github.com/aroseca15/Google-Books-Search#readme"
}
